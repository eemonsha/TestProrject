
@model TestProrject.Models.Supplier
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <div class="card card-primary mt-5">
        <div class="card-header">
            <h3 class="card-title">Unit Create</h3>
        </div>
        
        <form asp-action="SupplierCrt">
            <div class="card-body">
                <div class="form-group">
                    <label for="exampleInputPassword1">Supplier Name</label>
                    <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Supplier Name" asp-for="SupplierNAme">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Supplier Phone Number</label>
                    <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Supplier Phno" asp-for="SupplierAddress">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Supplier Address</label>
                    <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Supplier Address" asp-for="SNMbr">
                </div>
            </div>
            

            <div class="card-footer">
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
        </form>
    </div>
</body>
</html>
